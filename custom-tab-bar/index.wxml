<van-tabbar 
    active="{{ active }}" 
    bind:change="onChange" 
    active-color="#FFD000" 
    inactive-color="#666666"
    border="{{ false }}"
    safe-area-inset-bottom="{{ true }}">
    <van-tabbar-item 
        wx:for="{{ list }}" 
        wx:key="index"
        icon="{{ item.icon }}">
        {{item.text}}
    </van-tabbar-item>
</van-tabbar>

